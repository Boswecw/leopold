{
	"name": "leopold-frontend",
	"version": "1.0.0",
	"description": "Leopold Nature Observer - Modern wildlife observation and tracking application",
	"private": true,
	"type": "module",
	"keywords": [
		"nature",
		"wildlife",
		"observation",
		"conservation",
		"citizen-science",
		"audio",
		"svelte"
	],
	"author": "Leopold Development Team",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/your-org/leopold-frontend"
	},
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview --host",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:ui": "vitest --ui",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"type-check": "tsc --noEmit",
		"audio:test": "node scripts/test-audio.js",
		"build:analyze": "vite build --mode analyze",
		"preview:https": "vite preview --https --host"
	},
	"devDependencies": {
		"@rollup/rollup-linux-x64-gnu": "^4.46.2",
		"@sveltejs/adapter-auto": "^6.0.1",
		"@sveltejs/adapter-vercel": "^5.4.3",
		"@sveltejs/kit": "^1.0.0-next.589",
		"@sveltejs/vite-plugin-svelte": "^6.1.0",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.7",
		"@tailwindcss/typography": "^0.5.15",
		"@testing-library/jest-dom": "^6.4.8",
		"@testing-library/svelte": "^5.2.1",
		"@testing-library/user-event": "^14.5.2",
		"@types/leaflet": "^1.9.20",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^24.2.0",
		"@types/uuid": "^10.0.0",
		"@typescript-eslint/eslint-plugin": "^8.5.0",
		"@typescript-eslint/parser": "^8.5.0",
		"@vite-pwa/sveltekit": "^0.6.5",
		"autoprefixer": "^10.4.21",
		"cssnano": "^7.1.0",
		"eslint": "^9.10.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.43.0",
		"jsdom": "^25.0.0",
		"postcss": "^8.5.6",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"svelte": "4",
		"svelte-check": "^4.0.2",
		"tailwindcss": "^3.4.10",
		"typescript": "^5.9.2",
		"vite": "^4.5.3",
		"vite-plugin-pwa": "^1.0.2",
		"vitest": "^2.1.0",
		"workbox-window": "^7.1.0"
	},
	"dependencies": {
		"@floating-ui/dom": "^1.6.10",
		"@tensorflow/tfjs": "^4.21.0",
		"audio-buffer-utils": "^5.1.2",
		"clsx": "^2.1.1",
		"date-fns": "^3.6.0",
		"framer-motion": "^11.5.4",
		"js-cookie": "^3.0.5",
		"lamejs": "^1.2.1",
		"leaflet": "^1.9.4",
		"leaflet.markercluster": "^1.5.3",
		"lodash-es": "^4.17.21",
		"lucide-svelte": "^0.536.0",
		"recordrtc": "^5.6.2",
		"tailwind-merge": "^3.3.1",
		"uuid": "^10.0.0",
		"wavesurfer.js": "^7.10.1",
		"web-audio-api": "^0.2.2"
	},
	"engines": {
		"node": ">=18.0.0",
		"npm": ">=8.0.0"
	},
	"browserslist": [
		"defaults",
		"not IE 11",
		"not dead",
		"> 0.5%",
		"last 2 versions"
	],
	"pwa": {
		"name": "Leopold Nature Observer",
		"short_name": "Leopold",
		"description": "Track and discover wildlife through community science",
		"theme_color": "#2F5D50",
		"background_color": "#FAFAF9",
		"display": "standalone",
		"orientation": "portrait-primary",
		"scope": "/",
		"start_url": "/"
	}
}
